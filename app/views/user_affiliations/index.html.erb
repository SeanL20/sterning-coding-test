<p id="notice"><%= notice %></p>

<h1>User Affiliations</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Affiliation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_affiliations.each do |user_affiliation| %>
      <tr>
        <td><%= user_affiliation.user_id %></td>
        <td><%= user_affiliation.affiliation_id %></td>
        <td><%= link_to 'Show', user_affiliation %></td>
        <td><%= link_to 'Edit', edit_user_affiliation_path(user_affiliation) %></td>
        <td><%= link_to 'Destroy', user_affiliation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Affiliation', new_user_affiliation_path %>
